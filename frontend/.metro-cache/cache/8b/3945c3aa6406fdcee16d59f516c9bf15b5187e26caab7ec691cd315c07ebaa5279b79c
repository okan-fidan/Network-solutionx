Async({\n          accuracy: Location.Accuracy.High\n        });\n\n        // Adres al\n        let address = '';\n        try {\n          const [addressResult] = await Location.reverseGeocodeAsync({\n            latitude: location.coords.latitude,\n            longitude: location.coords.longitude\n          });\n          if (addressResult) {\n            address = [addressResult.street, addressResult.district, addressResult.city].filter(Boolean).join(', ');\n          }\n        } catch (e) {}\n\n        // Konum mesajÄ± gÃ¶nder\n        await _srcServicesApi.subgroupApi.sendMessage(groupId, {\n          content: `ðŸ“ ${address || 'Konum'}`,\n          type: 'location',\n          location: {\n            latitude: location.coords.latitude,\n            longitude: location.coords.longitude,\n            address,\n            isLive,\n            duration\n          }\n        });\n        loadData();\n        Alert.default.alert('BaÅŸarÄ±lÄ±', 'Konum paylaÅŸÄ±ldÄ±');\n      } catch (error) {\n        console.error('Error sharing location:', error);\n        Alert.default.alert('Hata', 'Konum paylaÅŸÄ±lamadÄ±');\n      } finally {\n        setLoadingLocation(false);\n        setShowLocationPicker(false);\n        setShowAttachMenu(false);\n      }\n    };\n\n    // Konum mesajÄ±nÄ± haritada aÃ§\n    const openLocationInMaps = (lat, lng) => {\n      const url = Platform.default.select({\n        ios: `maps:${lat},${lng}?q=Konum`,\n        android: `geo:${lat},${lng}?q=${lat},${lng}(Konum)`,\n        default: `https://maps.google.com/?q=${lat},${lng}`\n      });\n      Linking.default.openURL(url).catch(() => {\n        Linking.default.openURL(`https://maps.google.com/?q=${lat},${lng}`);\n      });\n    };\n\n    // Anket oluÅŸtur\n    const handleCreatePoll = async () => {\n      if (!pollQuestion.trim() || !groupId) return;\n      const validOptions = pollOptions.filter(o => o.trim());\n      if (validOptions.length < 2) {\n        Alert.default.alert('Hata', 'En