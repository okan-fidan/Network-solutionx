Replies(true);\n      try {\n        const response = await api.default.post('/api/ai/smart-replies', {\n          messageContent,\n          conversationType: 'group'\n        });\n        setSmartReplies(response.data.replies || []);\n      } catch (error) {\n        console.error('Error loading smart replies:', error);\n        setSmartReplies([]);\n      } finally {\n        setLoadingSmartReplies(false);\n      }\n    };\n\n    // AkÄ±llÄ± yanÄ±tÄ± seÃ§ ve gÃ¶nder\n    const handleSelectSmartReply = reply => {\n      setInputText(reply);\n      setSmartReplies([]);\n    };\n\n    // Ä°letme hedeflerini yÃ¼kle\n    const loadForwardTargets = async () => {\n      setLoadingForwardTargets(true);\n      try {\n        const [chatsRes, usersRes] = await Promise.all([api.default.get('/api/chats'), api.default.get('/api/users')]);\n        const targets = [];\n\n        // Sohbetleri ekle\n        chatsRes.data?.forEach(chat => {\n          targets.push({\n            id: chat.chatId,\n            name: chat.userName,\n            type: 'user'\n          });\n        });\n\n        // KullanÄ±cÄ±larÄ± ekle (henÃ¼z sohbet baÅŸlatÄ±lmamÄ±ÅŸ olanlar)\n        usersRes.data?.slice(0, 20).forEach(u => {\n          if (!targets.find(t => t.id.includes(u.uid))) {\n            targets.push({\n              id: u.uid,\n              name: `${u.firstName} ${u.lastName}`,\n              type: 'user'\n            });\n          }\n        });\n        setForwardTargets(targets);\n      } catch (error) {\n        console.error('Error loading forward targets:', error);\n      } finally {\n        setLoadingForwardTargets(false);\n      }\n    };\n    const uploadMedia = async (uri, type, fileName) => {\n      if (!user?.uid || !groupId) return null;\n      try {\n        const response = await fetch(uri);\n        const blob = await response.blob();\n        const ext = type === 'file' ? fileName?.split('.').pop() || 'file' : type === 'video' ? 'mp4' : 'jpg';\n        const path = 