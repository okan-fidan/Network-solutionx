nloadURL)(storageRef);\n      } catch (error) {\n        console.error('Error uploading media:', error);\n        return null;\n      }\n    };\n    const handleSend = async (mediaUrl, mediaType, fileName) => {\n      // D√ºzenleme modu\n      if (editingMessage) {\n        await handleEditMessage();\n        return;\n      }\n      if (!inputText.trim() && !mediaUrl || !groupId) return;\n\n      // Yazƒ±yor g√∂stergesini durdur\n      try {\n        await _srcServicesApi.chatStatusApi.sendTyping(groupId, false);\n      } catch (error) {\n        // Sessizce devam et\n      }\n      setSending(true);\n      setShowEmojiPicker(false);\n      setShowAttachMenu(false);\n      const messageText = inputText.trim();\n      setInputText('');\n      try {\n        const messageData = {\n          content: messageText,\n          type: mediaType || 'text'\n        };\n        if (mediaUrl) {\n          messageData.mediaUrl = mediaUrl;\n          if (fileName) messageData.fileName = fileName;\n        }\n\n        // Yanƒ±tlanan mesaj varsa ekle\n        if (replyingTo) {\n          messageData.replyTo = {\n            id: replyingTo.id,\n            content: replyingTo.content.substring(0, 100),\n            senderName: replyingTo.senderName\n          };\n        }\n        const response = await _srcServicesApi.subgroupApi.sendMessage(groupId, messageData);\n        setMessages([...messages, response.data]);\n        setReplyingTo(null);\n        setTimeout(() => flatListRef.current?.scrollToEnd(), 100);\n      } catch (error) {\n        console.error('Error sending message:', error);\n        setInputText(messageText);\n      } finally {\n        setSending(false);\n      }\n    };\n    const handleDeleteMessage = async (messageId, deleteForAll = false) => {\n      if (!groupId) return;\n      try {\n        await _srcServicesApi.subgroupApi.deleteMessage(groupId, messageId, deleteForAll);\n        if (deleteForAll) {\n          // Herkesten silindi - i√ßeriƒüi g√ºncelle\