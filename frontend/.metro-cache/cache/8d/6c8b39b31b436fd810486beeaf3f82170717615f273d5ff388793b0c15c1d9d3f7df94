Yazƒ±yor g√∂stergesi g√∂nder\n    const sendTypingIndicator = (0, _react.useCallback)(async () => {\n      if (!groupId) return;\n      const now = Date.now();\n      // Her 2 saniyede bir g√∂nder\n      if (now - lastTypingTime < 2000) return;\n      setLastTypingTime(now);\n      try {\n        await _srcServicesApi.chatStatusApi.sendTyping(groupId, true);\n      } catch (error) {\n        // Sessizce devam et\n      }\n    }, [groupId, lastTypingTime]);\n\n    // Input deƒüi≈ütiƒüinde yazƒ±yor g√∂stergesini g√∂nder\n    const handleInputChange = text => {\n      setInputText(text);\n      if (text.trim()) {\n        sendTypingIndicator();\n\n        // 3 saniye sonra yazƒ±yor g√∂stergesini durdur\n        if (typingSendTimeoutRef.current) {\n          clearTimeout(typingSendTimeoutRef.current);\n        }\n        typingSendTimeoutRef.current = setTimeout(async () => {\n          if (groupId) {\n            try {\n              await _srcServicesApi.chatStatusApi.sendTyping(groupId, false);\n            } catch (error) {\n              // Sessizce devam et\n            }\n          }\n        }, 3000);\n      }\n    };\n\n    // Son mesaj deƒüi≈ütiƒüinde akƒ±llƒ± yanƒ±tlarƒ± y√ºkle\n    (0, _react.useEffect)(() => {\n      const lastMessage = messages[messages.length - 1];\n      if (lastMessage && lastMessage.senderId !== user?.uid && lastMessage.type === 'text') {\n        loadSmartReplies(lastMessage.content);\n      } else {\n        setSmartReplies([]);\n      }\n    }, [messages]);\n\n    // Akƒ±llƒ± yanƒ±tlarƒ± y√ºkle\n    const loadSmartReplies = async messageContent => {\n      if (!messageContent || messageContent.length < 3) {\n        setSmartReplies([]);\n        return;\n      }\n      setLoadingSmartReplies(true);\n      try {\n        const response = await api.default.post('/api/ai/smart-replies', {\n          messageContent,\n          conversationType: 'group'\n        });\n        setSmartReplies(response.data.replies || []);\n      } catch (e