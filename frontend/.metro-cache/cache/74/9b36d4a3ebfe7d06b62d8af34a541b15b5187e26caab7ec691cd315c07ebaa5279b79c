) return;\n      try {\n        await _srcServicesApi.subgroupApi.deleteMessage(groupId, messageId, deleteForAll);\n        if (deleteForAll) {\n          // Herkesten silindi - iÃ§eriÄŸi gÃ¼ncelle\n          setMessages(messages.map(m => m.id === messageId ? Object.assign({}, m, {\n            content: 'Bu mesaj silindi',\n            type: 'deleted',\n            isDeleted: true\n          }) : m));\n        } else {\n          // Sadece benden silindi - listeden kaldÄ±r\n          setMessages(messages.filter(m => m.id !== messageId));\n        }\n      } catch (error) {\n        console.error('Error deleting message:', error);\n        Alert.default.alert('Hata', error?.response?.data?.detail || 'Mesaj silinemedi');\n      }\n      setShowMessageActions(false);\n    };\n    const showDeleteOptions = message => {\n      const isMe = message.senderId === user?.uid;\n      if (isMe) {\n        // Kendi mesajÄ±m - iki seÃ§enek gÃ¶ster\n        Alert.default.alert('MesajÄ± Sil', 'Bu mesajÄ± nasÄ±l silmek istiyorsunuz?', [{\n          text: 'Ä°ptal',\n          style: 'cancel'\n        }, {\n          text: 'Benden Sil',\n          onPress: () => handleDeleteMessage(message.id, false)\n        }, {\n          text: 'Herkesten Sil',\n          style: 'destructive',\n          onPress: () => handleDeleteMessage(message.id, true)\n        }]);\n      } else {\n        // BaÅŸkasÄ±nÄ±n mesajÄ± - sadece benden sil\n        Alert.default.alert('MesajÄ± Sil', 'Bu mesaj sizin iÃ§in silinecek.', [{\n          text: 'Ä°ptal',\n          style: 'cancel'\n        }, {\n          text: 'Sil',\n          style: 'destructive',\n          onPress: () => handleDeleteMessage(message.id, false)\n        }]);\n      }\n      setShowMessageActions(false);\n    };\n    const handleEditMessage = async () => {\n      if (!editingMessage || !inputText.trim() || !groupId) return;\n      try {\n        await _srcServicesApi.subgroupApi.editMessage(groupId, editingMessage.id, {\n          content: inputText.trim()\n        });\n        setMessages(messages.map(m => m.id === editingMessage.id ? Object.assign({}, m, {\n          content: inputText.trim(),\n          edited: true\n        }) : m));\n        setEditingMessage(null);\n        setInputText('');\n      } catch (error) {\n        console.error('Error editing message:', error?.response?.data || error);\n        Alert.default.alert('Hata', error?.response?.data?.detail || 'Mesaj dÃ¼zenlenemedi');\n      }\n    };\n    const startEditMessage = message => {\n      setEditingMessage(message);\n      setInputText(message.content);\n      setShowMessageActions(false);\n    };\n    const cancelEdit = () => {\n      setEditingMessage(null);\n      setInputText('');\n    };\n    const handlePinMessage = async message => {\n      if (!groupId) return;\n      try {\n        await _srcServicesApi.subgroupApi.pinMessage(groupId, message.id);\n        Alert.default.alert('BaÅŸarÄ±lÄ±', 'Mesaj sabitlendi');\n        setMessages(messages.map(m => m.id === message.id ? Object.assign({}, m, {\n          isPinned: true\n        }) : m));\n      } catch (error) {\n        console.error('Error pinning message:', error);\n        Alert.default.alert('Hata', error?.response?.data?.detail || 'Mesaj sabitlenemedi');\n      }\n      setShowMessageActions(false);\n    };\n\n    // YanÄ±tlama baÅŸlat\n    const startReply = message => {\n      setReplyingTo(message);\n      setShowMessageActions(false);\n    };\n\n    // YanÄ±tlamayÄ± iptal et\n    const cancelReply = () => {\n      setReplyingTo(null);\n    };\n\n    // Ä°letme baÅŸlat\n    const startForward = message => {\n      setForwardingMessage(message);\n      setShowMessageActions(false);\n      setShowForwardModal(true);\n      loadForwardTargets();\n    };\n\n    // Mesaj ilet\n    const handleForward = async (targetId, targetType) => {\n      if (!forwardingMessage) return;\n      try {\n        if (targetType === 'user') {\n          // Ã–zel mesaj olarak ilet\n 