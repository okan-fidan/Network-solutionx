      } catch (error) {\n        console.error('Error sharing location:', error);\n        Alert.default.alert('Hata', 'Konum paylaÅŸÄ±lamadÄ±');\n      } finally {\n        setLoadingLocation(false);\n        setShowLocationPicker(false);\n        setShowAttachMenu(false);\n      }\n    };\n\n    // Konum mesajÄ±nÄ± haritada aÃ§\n    const openLocationInMaps = (lat, lng) => {\n      const url = Platform.default.select({\n        ios: `maps:${lat},${lng}?q=Konum`,\n        android: `geo:${lat},${lng}?q=${lat},${lng}(Konum)`,\n        default: `https://maps.google.com/?q=${lat},${lng}`\n      });\n      Linking.default.openURL(url).catch(() => {\n        Linking.default.openURL(`https://maps.google.com/?q=${lat},${lng}`);\n      });\n    };\n    const formatTime = timestamp => {\n      try {\n        return (0, _dateFns.formatDistanceToNow)(new Date(timestamp), {\n          addSuffix: true,\n          locale: _dateFnsLocale.tr\n        });\n      } catch {\n        return '';\n      }\n    };\n\n    // Mesaj iÃ§eriÄŸini @mention highlight'lÄ± ÅŸekilde render et\n    const renderMessageContent = (content, isMe) => {\n      const mentionRegex = /@([A-Za-zÃ‡Ã§ÄžÄŸÄ°Ä±Ã–Ã¶ÅžÅŸÃœÃ¼]+\\s[A-Za-zÃ‡Ã§ÄžÄŸÄ°Ä±Ã–Ã¶ÅžÅŸÃœÃ¼]+)/g;\n      const parts = content.split(mentionRegex);\n      if (parts.length === 1) {\n        return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n          style: [styles.messageText, isMe && styles.myMessageText],\n          children: content\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 722,\n          columnNumber: 14\n        }, this);\n      }\n      const elements = [];\n      let match;\n      let lastIndex = 0;\n      const regex = new RegExp(mentionRegex);\n      while ((match = regex.exec(content)) !== null) {\n        if (match.index > lastIndex) {\n          elements.push(/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: [styles.messageText, isMe && styles