.filter(Boolean).join(', ');\n          }\n        } catch (e) {\n          // Adres alÄ±namazsa devam et\n        }\n        if (conversation) {\n          await _srcServicesApi.conversationApi.sendLocation(conversation.id, {\n            latitude: location.coords.latitude,\n            longitude: location.coords.longitude,\n            address\n          });\n          await loadMessages(conversation.id);\n        }\n      } catch (error) {\n        console.error('Error sending location:', error);\n        Alert.default.alert('Hata', 'Konum gÃ¶nderilemedi.');\n      } finally {\n        setSending(false);\n      }\n    };\n\n    // Konumu haritada aÃ§\n    const openLocationInMaps = (lat, lng) => {\n      const url = Platform.default.select({\n        ios: `maps:0,0?q=${lat},${lng}`,\n        android: `geo:${lat},${lng}?q=${lat},${lng}`\n      });\n      if (url) Linking.default.openURL(url);\n    };\n\n    // Mesaja uzun basma\n    const handleLongPress = message => {\n      if (message.type === 'deleted') return;\n      try {\n        Haptics.impactAsync(Haptics.ImpactFeedbackStyle.Medium);\n      } catch (e) {\n        Vibration.default.vibrate(50);\n      }\n      setSelectedMessage(message);\n      setShowMessageMenu(true);\n      Animated.default.spring(menuAnimation, {\n        toValue: 1,\n        useNativeDriver: true,\n        tension: 50,\n        friction: 7\n      }).start();\n    };\n    const closeMenu = () => {\n      Animated.default.timing(menuAnimation, {\n        toValue: 0,\n        duration: 150,\n        useNativeDriver: true\n      }).start(() => {\n        setShowMessageMenu(false);\n        setSelectedMessage(null);\n      });\n    };\n\n    // Emoji reaksiyon\n    const handleReaction = async emoji => {\n      if (!selectedMessage || !conversation?.id) {\n        console.log('Reaction failed: No conversation or message', {\n          conversation,\n          selectedMessage\n        });\n        Alert.default.alert('Hata', 'Reaksiyon eklenemedi');\n        closeMenu();\n        return;\n      }\n      try {\n        console.log('Adding reaction:', {\n          conversationId: conversation.id,\n          messageId: selectedMessage.id,\n          emoji\n        });\n        await _srcServicesApi.conversationApi.reactToMessage(conversation.id, selectedMessage.id, emoji);\n        setMessages(prev => prev.map(msg => {\n          if (msg.id === selectedMessage.id) {\n            const reactions = {\n              ...msg.reactions\n            };\n            const userId = user?.uid || '';\n            Object.keys(reactions).forEach(em => {\n              if (reactions[em]?.includes(userId)) {\n                reactions[em] = reactions[em].filter(id => id !== userId);\n                if (reactions[em].length === 0) delete reactions[em];\n              }\n            });\n            if (!reactions[emoji]) reactions[emoji] = [];\n            if (!reactions[emoji].includes(userId)) {\n              reactions[emoji].push(userId);\n            }\n            return {\n              ...msg,\n              reactions\n            };\n          }\n          return msg;\n        }));\n        closeMenu();\n      } catch (error) {\n        console.error('Error adding reaction:', error?.response?.data || error);\n        Alert.default.alert('Hata', 'Reaksiyon eklenemedi');\n        closeMenu();\n      }\n    };\n\n    // Mesaj silme\n    const handleDelete = async deleteForAll => {\n      if (!selectedMessage || !conversation) return;\n      Alert.default.alert('MesajÄ± Sil', deleteForAll ? 'Bu mesaj herkes iÃ§in silinecek.' : 'Bu mesaj sadece sizin iÃ§in silinecek.', [{\n        text: 'Ä°ptal',\n        style: 'cancel'\n      }, {\n        text: 'Sil',\n        style: 'destructive',\n        onPress: async () => {\n          try {\n            await _srcServicesApi.conversationApi.deleteMessage(conversation.id, selectedMessage.id, deleteForAll);\n            if (deleteForAll) {\n              setMessages(prev =>